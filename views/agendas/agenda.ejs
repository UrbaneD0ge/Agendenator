                <th colspan="3" style="text-align: center;font-family: Arial;size: 12pt; font-weight: bold;"><a
                    style="color:initial; text-decoration:initial"
                    href="https://www.atlantaga.gov/government/departments/city-planning/office-of-zoning-development/zoning/board-of-zoning-adjustment-bza"
                    board of zoning adjustment applications (bza)>Board of Zoning Adjustment Applications (BZA)</a></th>
              </tr>
              <tr style="font-size: 10pt;height: .2in;">
                <th style="border: .25pt solid black;text-align: center;font-family: Arial;size: 10pt;">
                  Application</th>
                <th style="border: .25pt solid black;text-align: center;font-family: Arial;size: 10pt;">
                  Property Address</th>
                <th style="border: .25pt solid black;text-align: center;font-family: Arial;size: 10pt;">
                  Public Hearing</th>
              </tr>
            </thead>
            <tbody>
              <!-- prototype application section -->
              <% applications.forEach(Application=> { %>
                <% if ( Application.type==='BZA' && Application.adjacent.includes(NPUs.NPU)) { %>
                  <tr>
                    <td style="text-align: left;font-family: Arial;size: 10pt;border: .25pt solid rgb(105, 105, 105);">
                      <a href="<%= Application.applURL %> ">
                        <%= Application.title %>
                      </a><br>
                      <%= Application.descr %>
                    </td>
                    <td
                      style="border-bottom: 1px solid black;text-align: center;font-family: Arial;size: 10pt;border: .25pt solid rgb(105, 105, 105);"
                      valign="top">
                      <%= Application.address %> (NPU-<%= Application.NPU %>)
                    </td>
                    <td
                      style="border-bottom: 1px solid black;text-align: center;font-family: Arial;size: 10pt;border: .25pt solid rgb(105, 105, 105);"
                      valign="top">
                      <%= Application.date %>
                    </td>
                  </tr>
                  <% } %>
                    <% }) %>
            </tbody>
          </table>
        </div>
        <div id="publicNotice">
          <br>
          <p
            style="text-align: center;font-family: 'Century Gothic';font-size: 18pt;margin-bottom: 0;break-after: avoid-page;">
            PUBLIC NOTICES AND ANNOUNCEMENTS</p>
          <p
            style="font-family: 'Century Gothic', CenturyGothic, AppleGothic, sans-serif;text-align: center;size: 10pt;margin: 0 20px 20px 20px;padding: 0 20px;">
            Provided for informational purposes. Votes/comments are neither required nor expected.</p>
        </div>
        <div>
          <% applications.forEach(Application=> { %>
            <% if ( Application.type==='Notice' && (Application.NPU===NPUs.NPU ||
              Application.adjacent.includes(NPUs.NPU))) { %>
              <table
                style="margin: 0 auto 25px auto;break-inside: avoid;text-align: center;font-family: Arial;size: 10pt;border: .25pt solid rgb(105, 105, 105);width: 100%;border-collapse: collapse;">
                <thead>
                  <tr style="background-color: rgb(200, 200, 200) !important;">
                    <th style="text-align: center;font-family: Arial;size: 12pt; font-weight: bold;">
                      <%= Application.title %>
                    </th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td style="text-align: left;font-family: Arial;size: 10pt;border: .25pt solid rgb(105, 105, 105);">
                      <%= Application.descr %>
                    </td>
                  </tr>
                </tbody>
              </table>
              <% } %>
                <% }) %>
        </div>
      </div>
    </div>
  </article>
</body>