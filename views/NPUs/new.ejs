<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>AGENDENATOR</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link type="text/css" rel="stylesheet" href="..\css\style.css">
</head>

<body class='container'>
  <header>
    <div>
      <div>
        <div class="col-md-10 text-center">
          <h1><a href='/'>AGENDENATOR</a></h1>
        </div>
      </div>
    </div>
  </header>
  <fieldset>
    <h2>Add NPU</h2>
    <form action="/NPUs" method="POST" autocomplete='off'>
      <div class="row">
        <div class="col-auto">
          <label for="NPU" class='col'>NPU</label>
          <select name="NPU" id="NPU" class='form-select form-select-lg' required>
            <option value='A'>A</option>
            <option value='B'>B</option>
            <option value='C'>C</option>
            <option value='D'>D</option>
            <option value='E'>E</option>
            <option value='F'>F</option>
            <option value='G'>G</option>
            <option value='H'>H</option>
            <option value='I'>I</option>
            <option value='J'>J</option>
            <option value='K'>K</option>
            <option value='L'>L</option>
            <option value='M'>M</option>
            <option value='N'>N</option>
            <option value='O'>O</option>
            <option value='P'>P</option>
            <option value='Q'>Q</option>
            <option value='R'>R</option>
            <option value='S'>S</option>
            <option value='T'>T</option>
            <option value='V'>V</option>
            <option value='W'>W</option>
            <option value='X'>X</option>
            <option value='Y'>Y</option>
            <option value='Z'>Z</option>
          </select>
        </div>
      </div>
      <div class='row'>
        <div class='col'>
          <label for="chair">Chair</label>
          <input type="text" name="chair" id="chair" class='form-control' required>
        </div>
        <div class='col'>
          <label for="chairE">Chair Email</label>
          <input type="email" name="chairE" id="chairE" class='form-control' autocomplete='off' required>
        </div>
      </div>
      <div class='row'>
        <div class='col'>
          <label for="Planner">Planner</label>
          <input type="text" name="planner" id="planner" class='form-control' autocomplete='off' required>
        </div>
        <div class='col'>
          <label for="PlannerE">Planner Email</label>
          <input type="email" name="plannerE" id="plannerE" class='form-control' required>
        </div>
      </div>
      <br><br>
      <div class='row'>
        <div class='col'>
          <label for="meeting">Meeting Recurrence & Time</label>
          <input type="text" name="meeting" id="meeting" class='form-control' required>
        </div>
        <div class='col'>
          <label for="ZoomURL">Zoom Meeting Link</label>
          <input type="text" name="ZoomURL" id="ZoomURL" class='form-control' required>
        </div>
      </div>
      <div class='row'>
        <div class='col'>
          <label for="ZoomID">Zoom Meeting ID</label>
          <input type="text" name="ZoomID" id="ZoomID" class='form-control' required>
        </div>
        <div class='col'>
          <label for="ZoomPW">Zoom Meeting Password</label>
          <input type="text" name="ZoomPW" id="ZoomPW" class='form-control' required>
        </div>
        <div class='col'>
          <label for="ZoomDial">Zoom Dial-In Number</label>
          <input type="text" name="ZoomDial" id="ZoomDial" class='form-control'>
        </div>
      </div>
      <br><br>
      <button class='btn btn-success' id='submit' type='submit'>SUBMIT</button>
    </form>
  </fieldset>
</body>